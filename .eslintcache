main.js",["82"],"import React, {\n    useState,\n    useEffect,\n    useCallback\n} from 'react';\nimport { withRouter } from 'react-router-dom';\nimport { Link } from 'react-router-dom';\nimport { GetAllRepo } from '../services';\nimport GetRandomId from '../getRandomId';\nimport ErrorIndicator from '../errorIndicator';\nimport Spinner from '../spinner';\n\nimport styled from \"styled-components\";\n\nconst Table = styled.table `\n    margin: 2rem auto;\n    border: 2px solid transparent;\n    border-image: linear-gradient(to right, red, purple);\n    border-image-slice: 1;\n`;\n\nconst Tr = styled.tr `\n    margin: 1.5rem;\n    display: ${(props) => (props.block ? \"block\" : \"table\")};\n`;\n\nconst Td = styled.td `\n    width: 200px;\n    word-break: break-all;\n    text-align: center;\n    vertical-align: middle;\n    font-weight: ${(props) => (props.name) ? \"bold\" : \"regular\"};\n    font-size: ${(props) => (props.name) ? \"20px\" : \"16px\"};\n`;\nconst Main = ({ history, match, valueSearch }) => {\n\n    const [ result, setResult ] = useState('');\n    const [ loading, toggleLoading ] = useState(true);\n    const [ error, toggleError ] = useState(false);\n\n    const { number } = match.params;\n    const numberPage = number ? number : 1;\n\n    const onError = () => {\n        toggleLoading(false);\n        toggleError(true);\n    }\n\n    useEffect(() => {\n        GetAllRepo(valueSearch, numberPage)\n            .then(results => {\n                setResult(results);\n                toggleLoading(false);\n                toggleError(false);\n            })\n            .catch(() => onError())\n    },[valueSearch, numberPage]);\n\n    const renderList = (arr) => {\n            const listRepo = arr.items.map((repo) => {\n                const key = GetRandomId();\n                return (\n                    <Tr key={key}>\n                        <Td>\n                            <Link \n                                to={{ pathname: `/repositories/${repo.name}`, query: { full: `${repo.full_name}` } }}\n                            >\n                                {repo.name}\n                            </Link>\n                        </Td>\n                        <Td>{repo.stargazers_count}</Td>\n                        <Td>{repo.pushed_at}</Td>\n                        <Td>\n                            <a href={repo.html_url}>{repo.html_url}</a>\n                        </Td>\n                    </Tr>\n                )\n            })\n            return listRepo;\n    }\n\n    const spinner = loading ? <Spinner /> : null;\n    const errorIndicator = error ? <ErrorIndicator /> : null;\n    const hasData = !(loading || error);\n    const content = hasData ? renderList(result) : null;\n\n    return (\n        <Table>\n            <tbody>\n                {errorIndicator}\n                {spinner}\n                {content}\n            </tbody>\n        </Table>\n    )\n}\n\nexport default withRouter(Main);","/Users/svetlanaromanova/Documents/HTML/Проекты/Обучение react/aviasales-app/src/app/index.js",[],"/Users/svetlanaromanova/Documents/HTML/Проекты/Обучение react/aviasales-app/src/pagination/index.js",[],"/Users/svetlanaromanova/Documents/HTML/Проекты/Обучение react/aviasales-app/src/pagination/pagination.js",[],"/Users/svetlanaromanova/Documents/HTML/Проекты/Обучение react/aviasales-app/src/getRandomId/index.js",[],"/Users/svetlanaromanova/Documents/HTML/Проекты/Обучение react/aviasales-app/src/services/getResources.js",["83"],"/Users/svetlanaromanova/Documents/HTML/Проекты/Обучение react/aviasales-app/src/spinner/index.js",[],["84","85"],"/Users/svetlanaromanova/Documents/HTML/Проекты/Обучение react/aviasales-app/src/spinner/spinner.js",[],"/Users/svetlanaromanova/Documents/HTML/Проекты/Обучение react/aviasales-app/src/errorIndicator/errorIndicator.js",[],"/Users/svetlanaromanova/Documents/HTML/Проекты/Обучение react/aviasales-app/src/errorIndicator/index.js",[],{"ruleId":"86","replacedBy":"87"},{"r